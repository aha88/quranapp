{
    "data":
        {
        "index": "90",
        "name": "Al-Balad",
        "verse": [

            {
                "text": "لَآ أُقْسِمُ بِهَٰذَا ٱلْبَلَدِ",
                "number": "",
                "verse": "1",
                "audioID": "v1",
                "url": "090001.mp3"
            },
            {
                "text": "وَأَنتَ حِلٌّۢ بِهَٰذَا ٱلْبَلَدِ",
                "number": "",
                "verse": "2",
                "audioID": "v2",
                "url": "090002.mp3"
            },
            {
                "text": "وَوَالِدٍۢ وَمَا وَلَدَ",
                "number": "",
                "verse": "3",
                "audioID": "v3",
                "url": "090003.mp3"
            },
            {
                "text": "لَقَدْ خَلَقْنَا ٱلْإِنسَٰنَ فِى كَبَدٍ",
                "number": "",
                "verse": "4",
                "audioID": "v4",
                "url": "090004.mp3"
            },
            {
                "text": "أَيَحْسَبُ أَن لَّن يَقْدِرَ عَلَيْهِ أَحَدٌۭ",
                "number": "",
                "verse": "5",
                "audioID": "v5",
                "url": "090005.mp3"
            },
            {
                "text": "يَقُولُ أَهْلَكْتُ مَالًۭا لُّبَدًا",
                "number": "",
                "verse": "6",
                "audioID": "v6",
                "url": "090006.mp3"
            },
            {
                "text": "أَيَحْسَبُ أَن لَّمْ يَرَهُۥٓ أَحَدٌ",
                "number": "",
                "verse": "7",
                "audioID": "v7",
                "url": "090007.mp3"
            },
            {
                "text": "أَلَمْ نَجْعَل لَّهُۥ عَيْنَيْنِ",
                "number": "",
                "verse": "8",
                "audioID": "v8",
                "url": "090008.mp3"
            },
            {
                "text": "وَلِسَانًۭا وَشَفَتَيْنِ",
                "number": "",
                "verse": "9",
                "audioID": "v9",
                "url": "090009.mp3"
            },
            {
                "text": "وَهَدَيْنَٰهُ ٱلنَّجْدَيْنِ",
                "number": "",
                "verse": "10",
                "audioID": "v10",
                "url": "090010.mp3"
            },
            {
                "text": "فَلَا ٱقْتَحَمَ ٱلْعَقَبَةَ",
                "number": "",
                "verse": "11",
                "audioID": "v11",
                "url": "090011.mp3"
            },
            {
                "text": "وَمَآ أَدْرَىٰكَ مَا ٱلْعَقَبَةُ",
                "number": "",
                "verse": "12",
                "audioID": "v12",
                "url": "090012.mp3"
            },
            {
                "text": "فَكُّ رَقَبَةٍ",
                "number": "",
                "verse": "13",
                "audioID": "v13",
                "url": "090013.mp3"
            },
            {
                "text": "أَوْ إِطْعَٰمٌۭ فِى يَوْمٍۢ ذِى مَسْغَبَةٍۢ",
                "number": "",
                "verse": "14",
                "audioID": "v14",
                "url": "090014.mp3"
            },
            {
                "text": "يَتِيمًۭا ذَا مَقْرَبَةٍ",
                "number": "",
                "verse": "15",
                "audioID": "v15",
                "url": "090015.mp3"
            },
            {
                "text": "أَوْ مِسْكِينًۭا ذَا مَتْرَبَةٍۢ",
                "number": "",
                "verse": "16",
                "audioID": "v16",
                "url": "090016.mp3"
            },
            {
                "text": "ثُمَّ كَانَ مِنَ ٱلَّذِينَ ءَامَنُوا۟ وَتَوَاصَوْا۟ بِٱلصَّبْرِ وَتَوَاصَوْا۟ بِٱلْمَرْحَمَةِ",
                "number": "",
                "verse": "17",
                "audioID": "v17",
                "url": "090017.mp3"
            },
            {
                "text": "أُو۟لَٰٓئِكَ أَصْحَٰبُ ٱلْمَيْمَنَةِ",
                "number": "",
                "verse": "18",
                "audioID": "v18",
                "url": "090018.mp3"
            },
            {
                "text": "وَٱلَّذِينَ كَفَرُوا۟ بِـَٔايَٰتِنَا هُمْ أَصْحَٰبُ ٱلْمَشْـَٔمَةِ",
                "number": "",
                "verse": "19",
                "audioID": "v19",
                "url": "090019.mp3"
            },
            {
                "text": "عَلَيْهِمْ نَارٌۭ مُّؤْصَدَةٌۢ",
                "number": "",
                "verse": "20",
                "audioID": "v20",
                "url": "090020.mp3"
            }

        ],
        "count": 20,
        "juz": [
            {
                "index": "30",
                "verse": {
                    "start": "verses_1",
                    "end": "verses_20"
                }
            }
        ]
    }
}