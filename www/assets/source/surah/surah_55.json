        "index": "55",
        "name": "Ar-Rahman",
        "verse": [
 
            {
                "text": " ٱلرَّحْمَٰنُ",
                "number": "",
                "verse": "1",
                "audioID": "v1",
                "url": "055001.mp3"
            },
            {
                "text": " عَلَّمَ ٱلْقُرْءَانَ",
                "number": "",
                "verse": "2",
                "audioID": "v2",
                "url": "055002.mp3"
            },
            {
                "text": " خَلَقَ ٱلْإِنسَٰنَ",
                "number": "",
                "verse": "3",
                "audioID": "v3",
                "url": "055003.mp3"
            },
            {
                "text": " عَلَّمَهُ ٱلْبَيَانَ",
                "number": "",
                "verse": "4",
                "audioID": "v4",
                "url": "055004.mp3"
            },
            {
                "text": " ٱلشَّمْسُ وَٱلْقَمَرُ بِحُسْبَانٍۢ",
                "number": "",
                "verse": "5",
                "audioID": "v5",
                "url": "055005.mp3"
            },
            {
                "text": " وَٱلنَّجْمُ وَٱلشَّجَرُ يَسْجُدَانِ",
                "number": "",
                "verse": "6",
                "audioID": "v6",
                "url": "055006.mp3"
            },
            {
                "text": " وَٱلسَّمَآءَ رَفَعَهَا وَوَضَعَ ٱلْمِيزَانَ",
                "number": "",
                "verse": "7",
                "audioID": "v7",
                "url": "055007.mp3"
            },
            {
                "text": " أَلَّا تَطْغَوْا۟ فِى ٱلْمِيزَانِ",
                "number": "",
                "verse": "8",
                "audioID": "v8",
                "url": "055008.mp3"
            },
            {
                "text": " وَأَقِيمُوا۟ ٱلْوَزْنَ بِٱلْقِسْطِ وَلَا تُخْسِرُوا۟ ٱلْمِيزَانَ",
                "number": "",
                "verse": "9",
                "audioID": "v9",
                "url": "055009.mp3"
            },
            {
                "text": " وَٱلْأَرْضَ وَضَعَهَا لِلْأَنَامِ",
                "number": "",
                "verse": "10",
                "audioID": "v10",
                "url": "055010.mp3"
            },
            {
                "text": " فِيهَا فَٰكِهَةٌۭ وَٱلنَّخْلُ ذَاتُ ٱلْأَكْمَامِ",
                "number": "",
                "verse": "11",
                "audioID": "v11",
                "url": "055011.mp3"
            },
            {
                "text": " وَٱلْحَبُّ ذُو ٱلْعَصْفِ وَٱلرَّيْحَانُ",
                "number": "",
                "verse": "12",
                "audioID": "v12",
                "url": "055012.mp3"
            },
            {
                "text": " فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
                "number": "",
                "verse": "13",
                "audioID": "v13",
                "url": "055013.mp3"
            },
            {
                "text": " خَلَقَ ٱلْإِنسَٰنَ مِن صَلْصَٰلٍۢ كَٱلْفَخَّارِ",
                "number": "",
                "verse": "14",
                "audioID": "v14",
                "url": "055014.mp3"
            },
            {
                "text": " وَخَلَقَ ٱلْجَآنَّ مِن مَّارِجٍۢ مِّن نَّارٍۢ",
                "number": "",
                "verse": "15",
                "audioID": "v15",
                "url": "055015.mp3"
            },
            {
                "text": " فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
                "number": "",
                "verse": "16",
                "audioID": "v16",
                "url": "055016.mp3"
            },
            {
                "text": " رَبُّ ٱلْمَشْرِقَيْنِ وَرَبُّ ٱلْمَغْرِبَيْنِ",
                "number": "",
                "verse": "17",
                "audioID": "v17",
                "url": "055017.mp3"
            },
            {
                "text": " فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
                "number": "",
                "verse": "18",
                "audioID": "v18",
                "url": "055018.mp3"
            },
            {
                "text": " مَرَجَ ٱلْبَحْرَيْنِ يَلْتَقِيَانِ",
                "number": "",
                "verse": "19",
                "audioID": "v19",
                "url": "055019.mp3"
            },
            {
                "text": " بَيْنَهُمَا بَرْزَخٌۭ لَّا يَبْغِيَانِ",
                "number": "",
                "verse": "20",
                "audioID": "v20",
                "url": "055020.mp3"
            },
            {
                "text": " فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
                "number": "",
                "verse": "21",
                "audioID": "v21",
                "url": "055021.mp3"
            },
            {
                "text": " يَخْرُجُ مِنْهُمَا ٱللُّؤْلُؤُ وَٱلْمَرْجَانُ",
                "number": "",
                "verse": "22",
                "audioID": "v22",
                "url": "055022.mp3"
            },
            {
                "text": " فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
                "number": "",
                "verse": "23",
                "audioID": "v23",
                "url": "055023.mp3"
            },
            {
                "text": " وَلَهُ ٱلْجَوَارِ ٱلْمُنشَـَٔاتُ فِى ٱلْبَحْرِ كَٱلْأَعْلَٰمِ",
                "number": "",
                "verse": "24",
                "audioID": "v24",
                "url": "055024.mp3"
            },
            {
                "text": " فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
                "number": "",
                "verse": "25",
                "audioID": "v25",
                "url": "055025.mp3"
            },
            {
                "text": " كُلُّ مَنْ عَلَيْهَا فَانٍۢ",
                "number": "",
                "verse": "26",
                "audioID": "v26",
                "url": "055026.mp3"
            },
            {
                "text": " وَيَبْقَىٰ وَجْهُ رَبِّكَ ذُو ٱلْجَلَٰلِ وَٱلْإِكْرَامِ",
                "number": "",
                "verse": "27",
                "audioID": "v27",
                "url": "055027.mp3"
            },
            {
                "text": " فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
                "number": "",
                "verse": "28",
                "audioID": "v28",
                "url": "055028.mp3"
            },
            {
                "text": " يَسْـَٔلُهُۥ مَن فِى ٱلسَّمَٰوَٰتِ وَٱلْأَرْضِ ۚ كُلَّ يَوْمٍ هُوَ فِى شَأْنٍۢ",
                "number": "",
                "verse": "29",
                "audioID": "v29",
                "url": "055029.mp3"
            },
            {
                "text": " فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
                "number": "",
                "verse": "30",
                "audioID": "v30",
                "url": "055030.mp3"
            },
            {
                "text": " سَنَفْرُغُ لَكُمْ أَيُّهَ ٱلثَّقَلَانِ",
                "number": "",
                "verse": "31",
                "audioID": "v31",
                "url": "055031.mp3"
            },
            {
                "text": " فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
                "number": "",
                "verse": "32",
                "audioID": "v32",
                "url": "055032.mp3"
            },
            {
                "text": " يَٰمَعْشَرَ ٱلْجِنِّ وَٱلْإِنسِ إِنِ ٱسْتَطَعْتُمْ أَن تَنفُذُوا۟ مِنْ أَقْطَارِ ٱلسَّمَٰوَٰتِ وَٱلْأَرْضِ فَٱنفُذُوا۟ ۚ لَا تَنفُذُونَ إِلَّا بِسُلْطَٰنٍۢ",
                "number": "",
                "verse": "33",
                "audioID": "v33",
                "url": "055033.mp3"
            },
            {
                "text": " فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
                "number": "",
                "verse": "34",
                "audioID": "v34",
                "url": "055034.mp3"
            },
            {
                "text": " يُرْسَلُ عَلَيْكُمَا شُوَاظٌۭ مِّن نَّارٍۢ وَنُحَاسٌۭ فَلَا تَنتَصِرَانِ",
                "number": "",
                "verse": "35",
                "audioID": "v35",
                "url": "055035.mp3"
            },
            {
                "text": " فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
                "number": "",
                "verse": "36",
                "audioID": "v36",
                "url": "055036.mp3"
            },
            {
                "text": " فَإِذَا ٱنشَقَّتِ ٱلسَّمَآءُ فَكَانَتْ وَرْدَةًۭ كَٱلدِّهَانِ",
                "number": "",
                "verse": "37",
                "audioID": "v37",
                "url": "055037.mp3"
            },
            {
                "text": " فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
                "number": "",
                "verse": "38",
                "audioID": "v38",
                "url": "055038.mp3"
            },
            {
                "text": " فَيَوْمَئِذٍۢ لَّا يُسْـَٔلُ عَن ذَنۢبِهِۦٓ إِنسٌۭ وَلَا جَآنٌّۭ",
                "number": "",
                "verse": "39",
                "audioID": "v39",
                "url": "055039.mp3"
            },
            {
                "text": " فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
                "number": "",
                "verse": "40",
                "audioID": "v40",
                "url": "055040.mp3"
            },
            {
                "text": " يُعْرَفُ ٱلْمُجْرِمُونَ بِسِيمَٰهُمْ فَيُؤْخَذُ بِٱلنَّوَٰصِى وَٱلْأَقْدَامِ",
                "number": "",
                "verse": "41",
                "audioID": "v41",
                "url": "055041.mp3"
            },
            {
                "text": " فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
                "number": "",
                "verse": "42",
                "audioID": "v42",
                "url": "055042.mp3"
            },
            {
                "text": " هَٰذِهِۦ جَهَنَّمُ ٱلَّتِى يُكَذِّبُ بِهَا ٱلْمُجْرِمُونَ",
                "number": "",
                "verse": "43",
                "audioID": "v43",
                "url": "055043.mp3"
            },
            {
                "text": " يَطُوفُونَ بَيْنَهَا وَبَيْنَ حَمِيمٍ ءَانٍۢ",
                "number": "",
                "verse": "44",
                "audioID": "v44",
                "url": "055044.mp3"
            },
            {
                "text": " فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
                "number": "",
                "verse": "45",
                "audioID": "v45",
                "url": "055045.mp3"
            },
            {
                "text": " وَلِمَنْ خَافَ مَقَامَ رَبِّهِۦ جَنَّتَانِ",
                "number": "",
                "verse": "46",
                "audioID": "v46",
                "url": "055046.mp3"
            },
            {
                "text": " فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
                "number": "",
                "verse": "47",
                "audioID": "v47",
                "url": "055047.mp3"
            },
            {
                "text": " ذَوَاتَآ أَفْنَانٍۢ",
                "number": "",
                "verse": "48",
                "audioID": "v48",
                "url": "055048.mp3"
            },
            {
                "text": " فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
                "number": "",
                "verse": "49",
                "audioID": "v49",
                "url": "055049.mp3"
            },
            {
                "text": " فِيهِمَا عَيْنَانِ تَجْرِيَانِ",
                "number": "",
                "verse": "50",
                "audioID": "v50",
                "url": "055050.mp3"
            },
            {
                "text": " فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
                "number": "",
                "verse": "51",
                "audioID": "v51",
                "url": "055051.mp3"
            },
            {
                "text": " فِيهِمَا مِن كُلِّ فَٰكِهَةٍۢ زَوْجَانِ",
                "number": "",
                "verse": "52",
                "audioID": "v52",
                "url": "055052.mp3"
            },
            {
                "text": " فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
                "number": "",
                "verse": "53",
                "audioID": "v53",
                "url": "055053.mp3"
            },
            {
                "text": " مُتَّكِـِٔينَ عَلَىٰ فُرُشٍۭ بَطَآئِنُهَا مِنْ إِسْتَبْرَقٍۢ ۚ وَجَنَى ٱلْجَنَّتَيْنِ دَانٍۢ",
                "number": "",
                "verse": "54",
                "audioID": "v54",
                "url": "055054.mp3"
            },
            {
                "text": " فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
                "number": "",
                "verse": "55",
                "audioID": "v55",
                "url": "055055.mp3"
            },
            {
                "text": " فِيهِنَّ قَٰصِرَٰتُ ٱلطَّرْفِ لَمْ يَطْمِثْهُنَّ إِنسٌۭ قَبْلَهُمْ وَلَا جَآنٌّۭ",
                "number": "",
                "verse": "56",
                "audioID": "v56",
                "url": "055056.mp3"
            },
            {
                "text": " فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
                "number": "",
                "verse": "57",
                "audioID": "v57",
                "url": "055057.mp3"
            },
            {
                "text": " كَأَنَّهُنَّ ٱلْيَاقُوتُ وَٱلْمَرْجَانُ",
                "number": "",
                "verse": "58",
                "audioID": "v58",
                "url": "055058.mp3"
            },
            {
                "text": " فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
                "number": "",
                "verse": "59",
                "audioID": "v59",
                "url": "055059.mp3"
            },
            {
                "text": " هَلْ جَزَآءُ ٱلْإِحْسَٰنِ إِلَّا ٱلْإِحْسَٰنُ",
