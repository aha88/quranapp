        "index": "101",
        "name": "Al-Qari'ah",
        "verse": [

            {
                "text": " ٱلْقَارِعَةُ",
                "number": "١",
                "verse": "1",
                "audioID": "v1",
                "url": "101001.mp3"
            },
            {
                "text": " مَا ٱلْقَارِعَةُ",
                "number": "٢",
                "verse": "2",
                "audioID": "v2",
                "url": "101002.mp3"
            },
            {
                "text": " وَمَآ أَدْرَىٰكَ مَا ٱلْقَارِعَةُ",
                "number": "٣",
                "verse": "3",
                "audioID": "v3",
                "url": "101003.mp3"
            },
            {
                "text": " يَوْمَ يَكُونُ ٱلنَّاسُ كَٱلْفَرَاشِ ٱلْمَبْثُوثِ",
                "number": "٤",
                "verse": "4",
                "audioID": "v4",
                "url": "101004.mp3"
            },
            {
                "text": " وَتَكُونُ ٱلْجِبَالُ كَٱلْعِهْنِ ٱلْمَنفُوشِ",
                "number": "٥",
                "verse": "5",
                "audioID": "v5",
                "url": "101005.mp3"
            },
            {
                "text": " فَأَمَّا مَن ثَقُلَتْ مَوَٰزِينُهُۥ",
                "number": "٦",
                "verse": "6",
                "audioID": "v6",
                "url": "101006.mp3"
            },
            {
                "text": " فَهُوَ فِى عِيشَةٍۢ رَّاضِيَةٍۢ",
                "number": "٧",
                "verse": "7",
                "audioID": "v7",
                "url": "101007.mp3"
            },
            {
                "text": " وَأَمَّا مَنْ خَفَّتْ مَوَٰزِينُهُۥ",
                "number": "٨",
                "verse": "8",
                "audioID": "v8",
                "url": "101008.mp3"
            },
            {
                "text": " فَأُمُّهُۥ هَاوِيَةٌۭ",
                "number": "٩",
                "verse": "9",
                "audioID": "v9",
                "url": "101009.mp3"
            },
            {
                "text": " وَمَآ أَدْرَىٰكَ مَا هِيَهْ",
                "number": "١٠",
                "verse": "10",
                "audioID": "v10",
                "url": "101010.mp3"
            },
            {
                "text": " نَارٌ حَامِيَةٌۢ",
                "number": "١١",
                "verse": "11",
                "audioID": "v11",
                "url": "101011.mp3"
            }

        ],
