{
    
    "data":
    {
        "index": "100",
        "name": "Al-'Adiyat ",
        "verse": [
            {
                "text": "وَٱلْعَٰدِيَٰتِ ضَبْحًۭا",
                "number": "١",
                "verse": "1",
                "audioID": "v1",
                "url": "100001.mp3"
            },
            {
                "text": "فَٱلْمُورِيَٰتِ قَدْحًۭا",
                "number": "٢",
                "verse": "2",
                "audioID": "v2",
                "url": "100002.mp3"
            },
            {
                "text": "فَٱلْمُغِيرَٰتِ صُبْحًۭا",
                "number": "٣",
                "verse": "3",
                "audioID": "v3",
                "url": "100003.mp3"
            },
            {
                "text": "فَأَثَرْنَ بِهِۦ نَقْعًۭا",
                "number": "٤",
                "verse": "4",
                "audioID": "v4",
                "url": "100004.mp3"
            },
            {
                "text": "فَوَسَطْنَ بِهِۦ جَمْعًا",
                "number": "٥",
                "verse": "5",
                "audioID": "v5",
                "url": "100005.mp3"
            },
            {
                "text": "إِنَّ ٱلْإِنسَٰنَ لِرَبِّهِۦ لَكَنُودٌۭ",
                "number": "٦",
                "verse": "6",
                "audioID": "v6",
                "url": "100006.mp3"
            },
            {
                "text": "وَإِنَّهُۥ عَلَىٰ ذَٰلِكَ لَشَهِيدٌۭ",
                "number": "٧",
                "verse": "7",
                "audioID": "v7",
                "url": "100007.mp3"
            },
            {
                "text": "وَإِنَّهُۥ لِحُبِّ ٱلْخَيْرِ لَشَدِيدٌ",
                "number": "٨",
                "verse": "8",
                "audioID": "v8",
                "url": "100008.mp3"
            },
            {
                "text": "۞ أَفَلَا يَعْلَمُ إِذَا بُعْثِرَ مَا فِى ٱلْقُبُورِ",
                "number": "٩",
                "verse": "9",
                "audioID": "v9",
                "url": "100009.mp3"
            },
            {
                "text": "وَحُصِّلَ مَا فِى ٱلصُّدُورِ",
                "number": "١٠",
                "verse": "10",
                "audioID": "v10",
                "url": "100010.mp3"
            },
            {
                "text": "إِنَّ رَبَّهُم بِهِمْ يَوْمَئِذٍۢ لَّخَبِيرٌۢ",
                "number": "١١",
                "verse": "11",
                "audioID": "v11",
                "url": "100011.mp3"
            }

        ],
        "count": 11,
        "juz": [
            {
                "index": "30",
                "verse": {
                    "start": "1",
                    "end": "11"
                }
            }
        ]
    }
}