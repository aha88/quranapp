{
    "data":
        {
        "index": "80",
        "name": "'Abasa",
        "verse": [

            {
                "text": " عَبَسَ وَتَوَلَّىٰٓ",
                "number": "",
                "verse": "1",
                "audioID": "v1",
                "url": "080001.mp3"
            },
            {
                "text": " أَن جَآءَهُ ٱلْأَعْمَىٰ",
                "number": "",
                "verse": "2",
                "audioID": "v2",
                "url": "080002.mp3"
            },
            {
                "text": " وَمَا يُدْرِيكَ لَعَلَّهُۥ يَزَّكَّىٰٓ",
                "number": "",
                "verse": "3",
                "audioID": "v3",
                "url": "080003.mp3"
            },
            {
                "text": " أَوْ يَذَّكَّرُ فَتَنفَعَهُ ٱلذِّكْرَىٰٓ",
                "number": "",
                "verse": "4",
                "audioID": "v4",
                "url": "080004.mp3"
            },
            {
                "text": " أَمَّا مَنِ ٱسْتَغْنَىٰ",
                "number": "",
                "verse": "5",
                "audioID": "v5",
                "url": "080005.mp3"
            },
            {
                "text": " فَأَنتَ لَهُۥ تَصَدَّىٰ",
                "number": "",
                "verse": "6",
                "audioID": "v6",
                "url": "080006.mp3"
            },
            {
                "text": " وَمَا عَلَيْكَ أَلَّا يَزَّكَّىٰ",
                "number": "",
                "verse": "7",
                "audioID": "v7",
                "url": "080007.mp3"
            },
            {
                "text": " وَأَمَّا مَن جَآءَكَ يَسْعَىٰ",
                "number": "",
                "verse": "8",
                "audioID": "v8",
                "url": "080008.mp3"
            },
            {
                "text": " وَهُوَ يَخْشَىٰ",
                "number": "",
                "verse": "9",
                "audioID": "v9",
                "url": "080009.mp3"
            },
            {
                "text": " فَأَنتَ عَنْهُ تَلَهَّىٰ",
                "number": "",
                "verse": "10",
                "audioID": "v10",
                "url": "080010.mp3"
            },
            {
                "text": " كَلَّآ إِنَّهَا تَذْكِرَةٌۭ",
                "number": "",
                "verse": "11",
                "audioID": "v11",
                "url": "080011.mp3"
            },
            {
                "text": " فَمَن شَآءَ ذَكَرَهُۥ",
                "number": "",
                "verse": "12",
                "audioID": "v12",
                "url": "080012.mp3"
            },
            {
                "text": " فِى صُحُفٍۢ مُّكَرَّمَةٍۢ",
                "number": "",
                "verse": "13",
                "audioID": "v13",
                "url": "080013.mp3"
            },
            {
                "text": " مَّرْفُوعَةٍۢ مُّطَهَّرَةٍۭ",
                "number": "",
                "verse": "14",
                "audioID": "v14",
                "url": "080014.mp3"
            },
            {
                "text": " بِأَيْدِى سَفَرَةٍۢ",
                "number": "",
                "verse": "15",
                "audioID": "v15",
                "url": "080015.mp3"
            },
            {
                "text": " كِرَامٍۭ بَرَرَةٍۢ",
                "number": "",
                "verse": "16",
                "audioID": "v16",
                "url": "080016.mp3"
            },
            {
                "text": " قُتِلَ ٱلْإِنسَٰنُ مَآ أَكْفَرَهُۥ",
                "number": "",
                "verse": "17",
                "audioID": "v17",
                "url": "080017.mp3"
            },
            {
                "text": " مِنْ أَىِّ شَىْءٍ خَلَقَهُۥ",
                "number": "",
                "verse": "18",
                "audioID": "v18",
                "url": "080018.mp3"
            },
            {
                "text": " مِن نُّطْفَةٍ خَلَقَهُۥ فَقَدَّرَهُۥ",
                "number": "",
                "verse": "19",
                "audioID": "v19",
                "url": "080019.mp3"
            },
            {
                "text": " ثُمَّ ٱلسَّبِيلَ يَسَّرَهُۥ",
                "number": "",
                "verse": "20",
                "audioID": "v20",
                "url": "080020.mp3"
            },
            {
                "text": " ثُمَّ أَمَاتَهُۥ فَأَقْبَرَهُۥ",
                "number": "",
                "verse": "21",
                "audioID": "v21",
                "url": "080021.mp3"
            },
            {
                "text": " ثُمَّ إِذَا شَآءَ أَنشَرَهُۥ",
                "number": "",
                "verse": "22",
                "audioID": "v22",
                "url": "080022.mp3"
            },
            {
                "text": " كَلَّا لَمَّا يَقْضِ مَآ أَمَرَهُۥ",
                "number": "",
                "verse": "23",
                "audioID": "v23",
                "url": "080023.mp3"
            },
            {
                "text": " فَلْيَنظُرِ ٱلْإِنسَٰنُ إِلَىٰ طَعَامِهِۦٓ",
                "number": "",
                "verse": "24",
                "audioID": "v24",
                "url": "080024.mp3"
            },
            {
                "text": " أَنَّا صَبَبْنَا ٱلْمَآءَ صَبًّۭا",
                "number": "",
                "verse": "25",
                "audioID": "v25",
                "url": "080025.mp3"
            },
            {
                "text": " ثُمَّ شَقَقْنَا ٱلْأَرْضَ شَقًّۭا",
                "number": "",
                "verse": "26",
                "audioID": "v26",
                "url": "080026.mp3"
            },
            {
                "text": " فَأَنۢبَتْنَا فِيهَا حَبًّۭا",
                "number": "",
                "verse": "27",
                "audioID": "v27",
                "url": "080027.mp3"
            },
            {
                "text": " وَعِنَبًۭا وَقَضْبًۭا",
                "number": "",
                "verse": "28",
                "audioID": "v28",
                "url": "080028.mp3"
            },
            {
                "text": " وَزَيْتُونًۭا وَنَخْلًۭا",
                "number": "",
                "verse": "29",
                "audioID": "v29",
                "url": "080029.mp3"
            },
            {
                "text": " وَحَدَآئِقَ غُلْبًۭا",
                "number": "",
                "verse": "30",
                "audioID": "v30",
                "url": "080030.mp3"
            },
            {
                "text": " وَفَٰكِهَةًۭ وَأَبًّۭا",
                "number": "",
                "verse": "31",
                "audioID": "v31",
                "url": "080031.mp3"
            },
            {
                "text": " مَّتَٰعًۭا لَّكُمْ وَلِأَنْعَٰمِكُمْ",
                "number": "",
                "verse": "32",
                "audioID": "v32",
                "url": "080032.mp3"
            },
            {
                "text": " فَإِذَا جَآءَتِ ٱلصَّآخَّةُ",
                "number": "",
                "verse": "33",
                "audioID": "v33",
                "url": "080033.mp3"
            },
            {
                "text": " يَوْمَ يَفِرُّ ٱلْمَرْءُ مِنْ أَخِيهِ",
                "number": "",
                "verse": "34",
                "audioID": "v34",
                "url": "080034.mp3"
            },
            {
                "text": " وَأُمِّهِۦ وَأَبِيهِ",
                "number": "",
                "verse": "35",
                "audioID": "v35",
                "url": "080035.mp3"
            },
            {
                "text": " وَصَٰحِبَتِهِۦ وَبَنِيهِ",
                "number": "",
                "verse": "36",
                "audioID": "v36",
                "url": "080036.mp3"
            },
            {
                "text": " لِكُلِّ ٱمْرِئٍۢ مِّنْهُمْ يَوْمَئِذٍۢ شَأْنٌۭ يُغْنِيهِ",
                "number": "",
                "verse": "37",
                "audioID": "v37",
                "url": "080037.mp3"
            },
            {
                "text": " وُجُوهٌۭ يَوْمَئِذٍۢ مُّسْفِرَةٌۭ",
                "number": "",
                "verse": "38",
                "audioID": "v38",
                "url": "080038.mp3"
            },
            {
                "text": " ضَاحِكَةٌۭ مُّسْتَبْشِرَةٌۭ",
                "number": "",
                "verse": "39",
                "audioID": "v39",
                "url": "080039.mp3"
            },
            {
                "text": " وَوُجُوهٌۭ يَوْمَئِذٍ عَلَيْهَا غَبَرَةٌۭ",
                "number": "",
                "verse": "40",
                "audioID": "v40",
                "url": "080040.mp3"
            },
            {
                "text": " تَرْهَقُهَا قَتَرَةٌ",
                "number": "",
                "verse": "41",
                "audioID": "v41",
                "url": "080041.mp3"
            },
            {
                "text": " أُو۟لَٰٓئِكَ هُمُ ٱلْكَفَرَةُ ٱلْفَجَرَةُ",
                "number": "",
                "verse": "42",
                "audioID": "v42",
                "url": "080042.mp3"
            }

        
        ],
