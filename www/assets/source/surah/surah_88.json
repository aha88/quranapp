{
    "data":
        {
        "index": "88",
        "name": "Al-Ghashiyah",
        "verse": [

            {
                "text": "هَلْ أَتَىٰكَ حَدِيثُ ٱلْغَٰشِيَةِ",
                "number": "",
                "verse": "1",
                "audioID": "v1",
                "url": "088001.mp3"
            },
            {
                "text": "وُجُوهٌۭ يَوْمَئِذٍ خَٰشِعَةٌ",
                "number": "",
                "verse": "2",
                "audioID": "v2",
                "url": "088002.mp3"
            },
            {
                "text": "عَامِلَةٌۭ نَّاصِبَةٌۭ",
                "number": "",
                "verse": "3",
                "audioID": "v3",
                "url": "088003.mp3"
            },
            {
                "text": "تَصْلَىٰ نَارًا حَامِيَةًۭ",
                "number": "",
                "verse": "4",
                "audioID": "v4",
                "url": "088004.mp3"
            },
            {
                "text": "تُسْقَىٰ مِنْ عَيْنٍ ءَانِيَةٍۢ",
                "number": "",
                "verse": "5",
                "audioID": "v5",
                "url": "088005.mp3"
            },
            {
                "text": "لَّيْسَ لَهُمْ طَعَامٌ إِلَّا مِن ضَرِيعٍۢ",
                "number": "",
                "verse": "6",
                "audioID": "v6",
                "url": "088006.mp3"
            },
            {
                "text": "لَّا يُسْمِنُ وَلَا يُغْنِى مِن جُوعٍۢ",
                "number": "",
                "verse": "7",
                "audioID": "v7",
                "url": "088007.mp3"
            },
            {
                "text": "وُجُوهٌۭ يَوْمَئِذٍۢ نَّاعِمَةٌۭ",
                "number": "",
                "verse": "8",
                "audioID": "v8",
                "url": "088008.mp3"
            },
            {
                "text": "لِّسَعْيِهَا رَاضِيَةٌۭ",
                "number": "",
                "verse": "9",
                "audioID": "v9",
                "url": "088009.mp3"
            },
            {
                "text": "فِى جَنَّةٍ عَالِيَةٍۢ",
                "number": "",
                "verse": "10",
                "audioID": "v10",
                "url": "088010.mp3"
            },
            {
                "text": "لَّا تَسْمَعُ فِيهَا لَٰغِيَةًۭ",
                "number": "",
                "verse": "11",
                "audioID": "v11",
                "url": "088011.mp3"
            },
            {
                "text": "فِيهَا عَيْنٌۭ جَارِيَةٌۭ",
                "number": "",
                "verse": "12",
                "audioID": "v12",
                "url": "088012.mp3"
            },
            {
                "text": "فِيهَا سُرُرٌۭ مَّرْفُوعَةٌۭ",
                "number": "",
                "verse": "13",
                "audioID": "v13",
                "url": "088013.mp3"
            },
            {
                "text": "وَأَكْوَابٌۭ مَّوْضُوعَةٌۭ",
                "number": "",
                "verse": "14",
                "audioID": "v14",
                "url": "088014.mp3"
            },
            {
                "text": "وَنَمَارِقُ مَصْفُوفَةٌۭ",
                "number": "",
                "verse": "15",
                "audioID": "v15",
                "url": "088015.mp3"
            },
            {
                "text": "وَزَرَابِىُّ مَبْثُوثَةٌ",
                "number": "",
                "verse": "16",
                "audioID": "v16",
                "url": "088016.mp3"
            },
            {
                "text": "أَفَلَا يَنظُرُونَ إِلَى ٱلْإِبِلِ كَيْفَ خُلِقَتْ",
                "number": "",
                "verse": "17",
                "audioID": "v17",
                "url": "088017.mp3"
            },
            {
                "text": "وَإِلَى ٱلسَّمَآءِ كَيْفَ رُفِعَتْ",
                "number": "",
                "verse": "18",
                "audioID": "v18",
                "url": "088018.mp3"
            },
            {
                "text": "وَإِلَى ٱلْجِبَالِ كَيْفَ نُصِبَتْ",
                "number": "",
                "verse": "19",
                "audioID": "v19",
                "url": "088019.mp3"
            },
            {
                "text": "وَإِلَى ٱلْأَرْضِ كَيْفَ سُطِحَتْ",
                "number": "",
                "verse": "20",
                "audioID": "v20",
                "url": "088020.mp3"
            },
            {
                "text": "فَذَكِّرْ إِنَّمَآ أَنتَ مُذَكِّرٌۭ",
                "number": "",
                "verse": "21",
                "audioID": "v21",
                "url": "088021.mp3"
            },
            {
                "text": "لَّسْتَ عَلَيْهِم بِمُصَيْطِرٍ",
                "number": "",
                "verse": "22",
                "audioID": "v22",
                "url": "088022.mp3"
            },
            {
                "text": "إِلَّا مَن تَوَلَّىٰ وَكَفَرَ",
                "number": "",
                "verse": "23",
                "audioID": "v23",
                "url": "088023.mp3"
            },
            {
                "text": "فَيُعَذِّبُهُ ٱللَّهُ ٱلْعَذَابَ ٱلْأَكْبَرَ",
                "number": "",
                "verse": "24",
                "audioID": "v24",
                "url": "088024.mp3"
            },
            {
                "text": "إِنَّ إِلَيْنَآ إِيَابَهُمْ",
                "number": "",
                "verse": "25",
                "audioID": "v25",
                "url": "088025.mp3"
            },
            {
                "text": "ثُمَّ إِنَّ عَلَيْنَا حِسَابَهُم",
                "number": "",
                "verse": "26",
                "audioID": "v26",
                "url": "088026.mp3"
            }

        ],
        "count": 26,
        "juz": [
            {
