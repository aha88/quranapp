        "index": "104",
        "name": "Al-Humazah",
        "verse": [
            {
                "text": " وَيْلٌۭ لِّكُلِّ هُمَزَةٍۢ لُّمَزَةٍ",
                "number": "١",
                "verse": "1",
                "audioID": "v1",
                "url": "104001.mp3"
            },
            {
                "text": " ٱلَّذِى جَمَعَ مَالًۭا وَعَدَّدَهُۥ",
                "number": "٢",
                "verse": "2",
                "audioID": "v2",
                "url": "104002.mp3"
            },
            {
                "text": " يَحْسَبُ أَنَّ مَالَهُۥٓ أَخْلَدَهُۥ",
                "number": "٣",
                "verse": "3",
                "audioID": "v3",
                "url": "104003.mp3"
            },
            {
                "text": " كَلَّا ۖ لَيُنۢبَذَنَّ فِى ٱلْحُطَمَةِ",
                "number": "٤",
                "verse": "4",
                "audioID": "v4",
                "url": "104004.mp3"
            },
            {
                "text": " وَمَآ أَدْرَىٰكَ مَا ٱلْحُطَمَةُ",
                "number": "٥",
                "verse": "5",
                "audioID": "v5",
                "url": "104005.mp3"
            },
            {
                "text": " نَارُ ٱللَّهِ ٱلْمُوقَدَةُ",
                "number": "٦",
                "verse": "6",
                "audioID": "v6",
                "url": "104006.mp3"
            },
            {
                "text": " ٱلَّتِى تَطَّلِعُ عَلَى ٱلْأَفْـِٔدَةِ",
                "number": "٧",
                "verse": "7",
                "audioID": "v7",
                "url": "104007.mp3"
            },
            {
                "text": " إِنَّهَا عَلَيْهِم مُّؤْصَدَةٌۭ",
                "number": "٨",
                "verse": "8",
                "audioID": "v8",
                "url": "104008.mp3"
            },
            {
                "text": " فِى عَمَدٍۢ مُّمَدَّدَةٍۭ",
                "number": "٩",
                "verse": "9",
                "audioID": "v9",
                "url": "104009.mp3"
            }

