{
    "data":
        {
        "index": "87",
        "name": "Al-A'la",
        "verse": [

            {
                "text": " سَبِّحِ ٱسْمَ رَبِّكَ ٱلْأَعْلَى",
                "number": "",
                "verse": "1",
                "audioID": "v1",
                "url": "087001.mp3"
            },
            {
                "text": " ٱلَّذِى خَلَقَ فَسَوَّىٰ",
                "number": "",
                "verse": "2",
                "audioID": "v2",
                "url": "087002.mp3"
            },
            {
                "text": " وَٱلَّذِى قَدَّرَ فَهَدَىٰ",
                "number": "",
                "verse": "3",
                "audioID": "v3",
                "url": "087003.mp3"
            },
            {
                "text": " وَٱلَّذِىٓ أَخْرَجَ ٱلْمَرْعَىٰ",
                "number": "",
                "verse": "4",
                "audioID": "v4",
                "url": "087004.mp3"
            },
            {
                "text": " فَجَعَلَهُۥ غُثَآءً أَحْوَىٰ",
                "number": "",
                "verse": "5",
                "audioID": "v5",
                "url": "087005.mp3"
            },
            {
                "text": " سَنُقْرِئُكَ فَلَا تَنسَىٰٓ",
                "number": "",
                "verse": "6",
                "audioID": "v6",
                "url": "087006.mp3"
            },
            {
                "text": " إِلَّا مَا شَآءَ ٱللَّهُ ۚ إِنَّهُۥ يَعْلَمُ ٱلْجَهْرَ وَمَا يَخْفَىٰ",
                "number": "",
                "verse": "7",
                "audioID": "v7",
                "url": "087007.mp3"
            },
            {
                "text": " وَنُيَسِّرُكَ لِلْيُسْرَىٰ",
                "number": "",
                "verse": "8",
                "audioID": "v8",
                "url": "087008.mp3"
            },
            {
                "text": " فَذَكِّرْ إِن نَّفَعَتِ ٱلذِّكْرَىٰ",
                "number": "",
                "verse": "9",
                "audioID": "v9",
                "url": "087009.mp3"
            },
            {
                "text": " سَيَذَّكَّرُ مَن يَخْشَىٰ",
                "number": "",
                "verse": "10",
                "audioID": "v10",
                "url": "087010.mp3"
            },
            {
                "text": " وَيَتَجَنَّبُهَا ٱلْأَشْقَى",
                "number": "",
                "verse": "11",
                "audioID": "v11",
                "url": "087011.mp3"
            },
            {
                "text": " ٱلَّذِى يَصْلَى ٱلنَّارَ ٱلْكُبْرَىٰ",
                "number": "",
                "verse": "12",
                "audioID": "v12",
                "url": "087012.mp3"
            },
            {
                "text": " ثُمَّ لَا يَمُوتُ فِيهَا وَلَا يَحْيَىٰ",
                "number": "",
                "verse": "13",
                "audioID": "v13",
                "url": "087013.mp3"
            },
            {
                "text": " قَدْ أَفْلَحَ مَن تَزَكَّىٰ",
                "number": "",
                "verse": "14",
                "audioID": "v14",
                "url": "087014.mp3"
            },
            {
                "text": " وَذَكَرَ ٱسْمَ رَبِّهِۦ فَصَلَّىٰ",
                "number": "",
                "verse": "15",
                "audioID": "v15",
                "url": "087015.mp3"
            },
            {
                "text": " بَلْ تُؤْثِرُونَ ٱلْحَيَوٰةَ ٱلدُّنْيَا",
                "number": "",
                "verse": "16",
                "audioID": "v16",
                "url": "087016.mp3"
            },
            {
                "text": " وَٱلْءَاخِرَةُ خَيْرٌۭ وَأَبْقَىٰٓ",
                "number": "",
                "verse": "17",
                "audioID": "v17",
                "url": "087017.mp3"
            },
            {
                "text": " إِنَّ هَٰذَا لَفِى ٱلصُّحُفِ ٱلْأُولَىٰ",
                "number": "",
                "verse": "18",
                "audioID": "v18",
                "url": "087018.mp3"
            },
            {
                "text": " صُحُفِ إِبْرَٰهِيمَ وَمُوسَىٰ",
                "number": "",
                "verse": "19",
                "audioID": "v19",
                "url": "087019.mp3"
            }

        ],
        "count": 19,
        "juz": [
            {
                "index": "30",
                "verse": {
                    "start": "verses_1",
                    "end": "verses_19"
                }
            }
        ]
    }
}