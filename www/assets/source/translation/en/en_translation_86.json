    "data":
        {
        "name": "The Nighcommer",
        "index": "86",
        "verse": [
             
            {
                "textEng": "By the sky and at-Tariq."
            },
            {
                "textEng": "But what will let you know what at-Tariq is?"
            },
            {
                "textEng": "The Piercing Star."
            },
            {
                "textEng": "There is no soul without a Protector over it."
            },
            {
                "textEng": "Let man consider what he was created from."
            },
            {
                "textEng": "He was created from gushing liquid."
            },
            {
                "textEng": "Issuing from between the backbone and the breastbones."
            },
            {
                "textEng": "He is certainly able to return him."
            },
            {
                "textEng": "On the Day when the secrets are disclosed."
            },
            {
                "textEng": "He will have no strength, and no supporter."
            },
            {
                "textEng": "By the sky that returns."
            },
            {
                "textEng": "And the earth that cracks open."
            },
            {
                "textEng": "It is a Decisive Word."
            },
            {
                "textEng": "It is no joke."
            },
            {
                "textEng": "They plot and scheme."
            },
            {
                "textEng": "But I plot and scheme."
            },
            {
                "textEng": "Therefore, give the blasphemers respite, a brief respite."
            }

        ],
        "count": 17
    }
}