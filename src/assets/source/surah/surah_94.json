        "name": "Ash-Sharh",
        "verse": [
            {
                "text": " أَلَمْ نَشْرَحْ لَكَ صَدْرَكَ",
                "number": "",
                "verse": "1",
                "audioID": "v1",
                "url": "094001.mp3"
            },
            {
                "text": " وَوَضَعْنَا عَنكَ وِزْرَكَ",
                "number": "",
                "verse": "2",
                "audioID": "v2",
                "url": "094002.mp3"
            },
            {
                "text": " ٱلَّذِىٓ أَنقَضَ ظَهْرَكَ",
                "number": "",
                "verse": "3",
                "audioID": "v3",
                "url": "094003.mp3"
            },
            {
                "text": " وَرَفَعْنَا لَكَ ذِكْرَكَ",
                "number": "",
                "verse": "4",
                "audioID": "v4",
                "url": "094004.mp3"
            },
            {
                "text": " فَإِنَّ مَعَ ٱلْعُسْرِ يُسْرًا",
                "number": "",
                "verse": "5",
                "audioID": "v5",
                "url": "094005.mp3"
            },
            {
                "text": " إِنَّ مَعَ ٱلْعُسْرِ يُسْرًۭا",
                "number": "",
                "verse": "6",
                "audioID": "v6",
                "url": "094006.mp3"
            },
            {
                "text": " فَإِذَا فَرَغْتَ فَٱنصَبْ",
                "number": "",
                "verse": "7",
                "audioID": "v7",
                "url": "094007.mp3"
            },
            {
                "text": " وَإِلَىٰ رَبِّكَ فَٱرْغَب",
