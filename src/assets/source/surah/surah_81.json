{
    "data":
        {
        "index": "81",
        "name": "At-Takwir",
        "verse": [
    
            {
                "text": " إِذَا ٱلشَّمْسُ كُوِّرَتْ",
                "number": "",
                "verse": "1",
                "audioID": "v1",
                "url": "081001.mp3"
            },
            {
                "text": " وَإِذَا ٱلنُّجُومُ ٱنكَدَرَتْ",
                "number": "",
                "verse": "2",
                "audioID": "v2",
                "url": "081002.mp3"
            },
            {
                "text": " وَإِذَا ٱلْجِبَالُ سُيِّرَتْ",
                "number": "",
                "verse": "3",
                "audioID": "v3",
                "url": "081003.mp3"
            },
            {
                "text": " وَإِذَا ٱلْعِشَارُ عُطِّلَتْ",
                "number": "",
                "verse": "4",
                "audioID": "v4",
                "url": "081004.mp3"
            },
            {
                "text": " وَإِذَا ٱلْوُحُوشُ حُشِرَتْ",
                "number": "",
                "verse": "5",
                "audioID": "v5",
                "url": "081005.mp3"
            },
            {
                "text": " وَإِذَا ٱلْبِحَارُ سُجِّرَتْ",
                "number": "",
                "verse": "6",
                "audioID": "v6",
                "url": "081006.mp3"
            },
            {
                "text": " وَإِذَا ٱلنُّفُوسُ زُوِّجَتْ",
                "number": "",
                "verse": "7",
                "audioID": "v7",
                "url": "081007.mp3"
            },
            {
                "text": " وَإِذَا ٱلْمَوْءُۥدَةُ سُئِلَتْ",
                "number": "",
                "verse": "8",
                "audioID": "v8",
                "url": "081008.mp3"
            },
            {
                "text": " بِأَىِّ ذَنۢبٍۢ قُتِلَتْ",
                "number": "",
                "verse": "9",
                "audioID": "v9",
                "url": "081009.mp3"
            },
            {
                "text": " وَإِذَا ٱلصُّحُفُ نُشِرَتْ",
                "number": "",
                "verse": "10",
                "audioID": "v10",
                "url": "081010.mp3"
            },
            {
                "text": " وَإِذَا ٱلسَّمَآءُ كُشِطَتْ",
                "number": "",
                "verse": "11",
                "audioID": "v11",
                "url": "081011.mp3"
            },
            {
                "text": " وَإِذَا ٱلْجَحِيمُ سُعِّرَتْ",
                "number": "",
                "verse": "12",
                "audioID": "v12",
                "url": "081012.mp3"
            },
            {
                "text": " وَإِذَا ٱلْجَنَّةُ أُزْلِفَتْ",
                "number": "",
                "verse": "13",
                "audioID": "v13",
                "url": "081013.mp3"
            },
            {
                "text": " عَلِمَتْ نَفْسٌۭ مَّآ أَحْضَرَتْ",
                "number": "",
                "verse": "14",
                "audioID": "v14",
                "url": "081014.mp3"
            },
            {
                "text": " فَلَآ أُقْسِمُ بِٱلْخُنَّسِ",
                "number": "",
                "verse": "15",
                "audioID": "v15",
                "url": "081015.mp3"
            },
            {
                "text": " ٱلْجَوَارِ ٱلْكُنَّسِ",
                "number": "",
                "verse": "16",
                "audioID": "v16",
                "url": "081016.mp3"
            },
            {
                "text": " وَٱلَّيْلِ إِذَا عَسْعَسَ",
                "number": "",
                "verse": "17",
                "audioID": "v17",
                "url": "081017.mp3"
            },
            {
                "text": " وَٱلصُّبْحِ إِذَا تَنَفَّسَ",
                "number": "",
                "verse": "18",
                "audioID": "v18",
                "url": "081018.mp3"
            },
            {
                "text": " إِنَّهُۥ لَقَوْلُ رَسُولٍۢ كَرِيمٍۢ",
                "number": "",
                "verse": "19",
                "audioID": "v19",
                "url": "081019.mp3"
            },
            {
                "text": " ذِى قُوَّةٍ عِندَ ذِى ٱلْعَرْشِ مَكِينٍۢ",
                "number": "",
                "verse": "20",
                "audioID": "v20",
                "url": "081020.mp3"
            },
            {
                "text": " مُّطَاعٍۢ ثَمَّ أَمِينٍۢ",
                "number": "",
                "verse": "21",
                "audioID": "v21",
                "url": "081021.mp3"
            },
            {
                "text": " وَمَا صَاحِبُكُم بِمَجْنُونٍۢ",
                "number": "",
                "verse": "22",
                "audioID": "v22",
                "url": "081022.mp3"
            },
            {
                "text": " وَلَقَدْ رَءَاهُ بِٱلْأُفُقِ ٱلْمُبِينِ",
                "number": "",
                "verse": "23",
                "audioID": "v23",
                "url": "081023.mp3"
            },
            {
                "text": " وَمَا هُوَ عَلَى ٱلْغَيْبِ بِضَنِينٍۢ",
                "number": "",
                "verse": "24",
                "audioID": "v24",
                "url": "081024.mp3"
            },
            {
                "text": " وَمَا هُوَ بِقَوْلِ شَيْطَٰنٍۢ رَّجِيمٍۢ",
                "number": "",
                "verse": "25",
                "audioID": "v25",
                "url": "081025.mp3"
            },
            {
                "text": " فَأَيْنَ تَذْهَبُونَ",
                "number": "",
                "verse": "26",
                "audioID": "v26",
                "url": "081026.mp3"
            },
            {
                "text": " إِنْ هُوَ إِلَّا ذِكْرٌۭ لِّلْعَٰلَمِينَ",
                "number": "",
                "verse": "27",
                "audioID": "v27",
                "url": "081027.mp3"
            },
            {
                "text": " لِمَن شَآءَ مِنكُمْ أَن يَسْتَقِيمَ",
                "number": "",
                "verse": "28",
                "audioID": "v28",
                "url": "081028.mp3"
            },
            {
                "text": " وَمَا تَشَآءُونَ إِلَّآ أَن يَشَآءَ ٱللَّهُ رَبُّ ٱلْعَٰلَمِينَ",
                "number": "",
                "verse": "29",
                "audioID": "v29",
                "url": "081029.mp3"
            }

        
        ],
        "count": 29,
        "juz": [
