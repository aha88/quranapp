{
    "data":
        {
        "index": "96",
        "name": "Al-'Alaq",
        "verse": [

            {
                "text": " ٱقْرَأْ بِٱسْمِ رَبِّكَ ٱلَّذِى خَلَقَ",
                "number": "",
                "verse": "1",
                "audioID": "v1",
                "url": "096001.mp3"
            },
            {
                "text": " خَلَقَ ٱلْإِنسَٰنَ مِنْ عَلَقٍ",
                "number": "",
                "verse": "2",
                "audioID": "v2",
                "url": "096002.mp3"
            },
            {
                "text": " ٱقْرَأْ وَرَبُّكَ ٱلْأَكْرَمُ",
                "number": "",
                "verse": "3",
                "audioID": "v3",
                "url": "096003.mp3"
            },
            {
                "text": " ٱلَّذِى عَلَّمَ بِٱلْقَلَمِ",
                "number": "",
                "verse": "4",
                "audioID": "v4",
                "url": "096004.mp3"
            },
            {
                "text": " عَلَّمَ ٱلْإِنسَٰنَ مَا لَمْ يَعْلَمْ",
                "number": "",
                "verse": "5",
                "audioID": "v5",
                "url": "096005.mp3"
            },
            {
                "text": " كَلَّآ إِنَّ ٱلْإِنسَٰنَ لَيَطْغَىٰٓ",
                "number": "",
                "verse": "6",
                "audioID": "v6",
                "url": "096006.mp3"
            },
            {
                "text": " أَن رَّءَاهُ ٱسْتَغْنَىٰٓ",
                "number": "",
                "verse": "7",
                "audioID": "v7",
                "url": "096007.mp3"
            },
            {
                "text": " إِنَّ إِلَىٰ رَبِّكَ ٱلرُّجْعَىٰٓ",
                "number": "",
                "verse": "8",
                "audioID": "v8",
                "url": "096008.mp3"
            },
            {
                "text": " أَرَءَيْتَ ٱلَّذِى يَنْهَىٰ",
                "number": "",
                "verse": "9",
                "audioID": "v9",
                "url": "096009.mp3"
            },
            {
                "text": " عَبْدًا إِذَا صَلَّىٰٓ",
                "number": "",
                "verse": "10",
                "audioID": "v10",
                "url": "096010.mp3"
            },
            {
                "text": " أَرَءَيْتَ إِن كَانَ عَلَى ٱلْهُدَىٰٓ",
                "number": "",
                "verse": "11",
                "audioID": "v11",
                "url": "096011.mp3"
            },
            {
                "text": " أَوْ أَمَرَ بِٱلتَّقْوَىٰٓ",
                "number": "",
                "verse": "12",
                "audioID": "v12",
                "url": "096012.mp3"
            },
            {
                "text": " أَرَءَيْتَ إِن كَذَّبَ وَتَوَلَّىٰٓ",
                "number": "",
                "verse": "13",
                "audioID": "v13",
                "url": "096013.mp3"
            },
            {
                "text": " أَلَمْ يَعْلَم بِأَنَّ ٱللَّهَ يَرَىٰ",
                "number": "",
                "verse": "14",
                "audioID": "v14",
                "url": "096014.mp3"
            },
            {
                "text": " كَلَّا لَئِن لَّمْ يَنتَهِ لَنَسْفَعًۢا بِٱلنَّاصِيَةِ",
                "number": "",
                "verse": "15",
                "audioID": "v15",
                "url": "096015.mp3"
            },
            {
                "text": " نَاصِيَةٍۢ كَٰذِبَةٍ خَاطِئَةٍۢ",
                "number": "",
                "verse": "16",
                "audioID": "v16",
                "url": "096016.mp3"
            },
            {
                "text": " فَلْيَدْعُ نَادِيَهُۥ",
                "number": "",
                "verse": "17",
                "audioID": "v17",
                "url": "096017.mp3"
            },
            {
                "text": " سَنَدْعُ ٱلزَّبَانِيَةَ",
                "number": "",
                "verse": "18",
                "audioID": "v18",
                "url": "096018.mp3"
            },
            {
                "text": " كَلَّا لَا تُطِعْهُ وَٱسْجُدْ وَٱقْتَرِب ۩",
                "number": "",
                "verse": "19",
                "audioID": "v19",
                "url": "096019.mp3"
            }
        ],
        "count": 19,
        "juz": [
            {
                "index": "30",
                "verse": {
                    "start": "1",
                    "end": "19"
                }
            }
        ]
    }
}