{
    "data":
        {
        "index": "86",
        "name": "At-Tariq",
        "verse": [
            {
                "text": " بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
                "number": "",
                "verse": "0",
                "audioID": "v0",
                "url": "086000.mp3"
            },
            {
                "text": " وَٱلسَّمَآءِ وَٱلطَّارِقِ",
                "number": "",
                "verse": "1",
                "audioID": "v1",
                "url": "086001.mp3"
            },
            {
                "text": " وَمَآ أَدْرَىٰكَ مَا ٱلطَّارِقُ",
                "number": "",
                "verse": "2",
                "audioID": "v2",
                "url": "086002.mp3"
            },
            {
                "text": " ٱلنَّجْمُ ٱلثَّاقِبُ",
                "number": "",
                "verse": "3",
                "audioID": "v3",
                "url": "086003.mp3"
            },
            {
                "text": " إِن كُلُّ نَفْسٍۢ لَّمَّا عَلَيْهَا حَافِظٌۭ",
                "number": "",
                "verse": "4",
                "audioID": "v4",
                "url": "086004.mp3"
            },
            {
                "text": " فَلْيَنظُرِ ٱلْإِنسَٰنُ مِمَّ خُلِقَ",
                "number": "",
                "verse": "5",
                "audioID": "v5",
                "url": "086005.mp3"
            },
            {
                "text": " خُلِقَ مِن مَّآءٍۢ دَافِقٍۢ",
                "number": "",
                "verse": "6",
                "audioID": "v6",
                "url": "086006.mp3"
            },
            {
                "text": " يَخْرُجُ مِنۢ بَيْنِ ٱلصُّلْبِ وَٱلتَّرَآئِبِ",
                "number": "",
                "verse": "7",
                "audioID": "v7",
                "url": "086007.mp3"
            },
            {
                "text": " إِنَّهُۥ عَلَىٰ رَجْعِهِۦ لَقَادِرٌۭ",
                "number": "",
                "verse": "8",
                "audioID": "v8",
                "url": "086008.mp3"
            },
            {
                "text": " يَوْمَ تُبْلَى ٱلسَّرَآئِرُ",
                "number": "",
                "verse": "9",
                "audioID": "v9",
                "url": "086009.mp3"
            },
            {
                "text": " فَمَا لَهُۥ مِن قُوَّةٍۢ وَلَا نَاصِرٍۢ",
                "number": "",
                "verse": "10",
                "audioID": "v10",
                "url": "086010.mp3"
            },
            {
                "text": " وَٱلسَّمَآءِ ذَاتِ ٱلرَّجْعِ",
                "number": "",
                "verse": "11",
                "audioID": "v11",
                "url": "086011.mp3"
            },
            {
                "text": " وَٱلْأَرْضِ ذَاتِ ٱلصَّدْعِ",
                "number": "",
                "verse": "12",
                "audioID": "v12",
                "url": "086012.mp3"
            },
            {
                "text": " إِنَّهُۥ لَقَوْلٌۭ فَصْلٌۭ",
                "number": "",
                "verse": "13",
                "audioID": "v13",
                "url": "086013.mp3"
            },
            {
                "text": " وَمَا هُوَ بِٱلْهَزْلِ",
                "number": "",
                "verse": "14",
                "audioID": "v14",
                "url": "086014.mp3"
            },
            {
                "text": " إِنَّهُمْ يَكِيدُونَ كَيْدًۭا",
                "number": "",
                "verse": "15",
                "audioID": "v15",
                "url": "086015.mp3"
            },
            {
                "text": " وَأَكِيدُ كَيْدًۭا",
                "number": "",
                "verse": "16",
                "audioID": "v16",
                "url": "086016.mp3"
            },
            {
                "text": " فَمَهِّلِ ٱلْكَٰفِرِينَ أَمْهِلْهُمْ رُوَيْدًۢا",
                "number": "",
                "verse": "17",
                "audioID": "v17",
                "url": "086017.mp3"
            }

        ],
        "count": 17,
        "juz": [
            {
                "index": "30",
                "verse": {
                    "start": "1",
                    "end": "17"
                }
            }
        ]
    }
}