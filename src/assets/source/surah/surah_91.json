{
    "data":
        {
        "index": "91",
        "name": "Ash-Shams",
        "verse": [

            {
                "text": "وَٱلشَّمْسِ وَضُحَىٰهَا",
                "number": "",
                "verse": "1",
                "audioID": "v1",
                "url": "091001.mp3"
            },
            {
                "text": "وَٱلْقَمَرِ إِذَا تَلَىٰهَا",
                "number": "",
                "verse": "2",
                "audioID": "v2",
                "url": "091002.mp3"
            },
            {
                "text": "وَٱلنَّهَارِ إِذَا جَلَّىٰهَا",
                "number": "",
                "verse": "3",
                "audioID": "v3",
                "url": "091003.mp3"
            },
            {
                "text": "وَٱلَّيْلِ إِذَا يَغْشَىٰهَا",
                "number": "",
                "verse": "4",
                "audioID": "v4",
                "url": "091004.mp3"
            },
            {
                "text": "وَٱلسَّمَآءِ وَمَا بَنَىٰهَا",
                "number": "",
                "verse": "5",
                "audioID": "v5",
                "url": "091005.mp3"
            },
            {
                "text": "وَٱلْأَرْضِ وَمَا طَحَىٰهَا",
                "number": "",
                "verse": "6",
                "audioID": "v6",
                "url": "091006.mp3"
            },
            {
                "text": "وَنَفْسٍۢ وَمَا سَوَّىٰهَا",
                "number": "",
                "verse": "7",
                "audioID": "v7",
                "url": "091007.mp3"
            },
            {
                "text": "فَأَلْهَمَهَا فُجُورَهَا وَتَقْوَىٰهَا",
                "number": "",
                "verse": "8",
                "audioID": "v8",
                "url": "091008.mp3"
            },
            {
                "text": "قَدْ أَفْلَحَ مَن زَكَّىٰهَا",
                "number": "",
                "verse": "9",
                "audioID": "v9",
                "url": "091009.mp3"
            },
            {
                "text": "وَقَدْ خَابَ مَن دَسَّىٰهَا",
                "number": "",
                "verse": "10",
                "audioID": "v10",
                "url": "091010.mp3"
            },
            {
                "text": "كَذَّبَتْ ثَمُودُ بِطَغْوَىٰهَآ",
                "number": "",
                "verse": "11",
                "audioID": "v11",
                "url": "091011.mp3"
            },
            {
                "text": "إِذِ ٱنۢبَعَثَ أَشْقَىٰهَا",
                "number": "",
                "verse": "12",
                "audioID": "v12",
                "url": "091012.mp3"
            },
            {
                "text": "فَقَالَ لَهُمْ رَسُولُ ٱللَّهِ نَاقَةَ ٱللَّهِ وَسُقْيَٰهَا",
                "number": "",
                "verse": "13",
                "audioID": "v13",
                "url": "091013.mp3"
            },
            {
                "text": "فَكَذَّبُوهُ فَعَقَرُوهَا فَدَمْدَمَ عَلَيْهِمْ رَبُّهُم بِذَنۢبِهِمْ فَسَوَّىٰهَا",
                "number": "",
                "verse": "14",
                "audioID": "v14",
                "url": "091014.mp3"
            },
            {
                "text": "وَلَا يَخَافُ عُقْبَٰهَا",
                "number": "",
                "verse": "15",
                "audioID": "v15",
                "url": "091015.mp3"
            }

        ],
        "count": 15,
        "juz": [
            {
                "index": "30",
                "verse": {
                    "start": "versse_1",
                    "end": "versse_15"
                }
            }
        ]
    }
}