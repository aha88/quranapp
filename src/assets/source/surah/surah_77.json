{
    "data":
        {
        "index": "77",
        "name": "Al-Mursalat",
        "verse": [
 
            {
                "text": "وَٱلْمُرْسَلَٰتِ عُرْفًۭا",
                "number": "",
                "verse": "1",
                "audioID": "v1",
                "url": "077001.mp3"
            },
            {
                "text": "فَٱلْعَٰصِفَٰتِ عَصْفًۭا",
                "number": "",
                "verse": "2",
                "audioID": "v2",
                "url": "077002.mp3"
            },
            {
                "text": "وَٱلنَّٰشِرَٰتِ نَشْرًۭا",
                "number": "",
                "verse": "3",
                "audioID": "v3",
                "url": "077003.mp3"
            },
            {
                "text": "فَٱلْفَٰرِقَٰتِ فَرْقًۭا",
                "number": "",
                "verse": "4",
                "audioID": "v4",
                "url": "077004.mp3"
            },
            {
                "text": "فَٱلْمُلْقِيَٰتِ ذِكْرًا",
                "number": "",
                "verse": "5",
                "audioID": "v5",
                "url": "077005.mp3"
            },
            {
                "text": "عُذْرًا أَوْ نُذْرًا",
                "number": "",
                "verse": "6",
                "audioID": "v6",
                "url": "077006.mp3"
            },
            {
                "text": "إِنَّمَا تُوعَدُونَ لَوَٰقِعٌۭ",
                "number": "",
                "verse": "7",
                "audioID": "v7",
                "url": "077007.mp3"
            },
            {
                "text": "فَإِذَا ٱلنُّجُومُ طُمِسَتْ",
                "number": "",
                "verse": "8",
                "audioID": "v8",
                "url": "077008.mp3"
            },
            {
                "text": "وَإِذَا ٱلسَّمَآءُ فُرِجَتْ",
                "number": "",
                "verse": "9",
                "audioID": "v9",
                "url": "077009.mp3"
            },
            {
                "text": "وَإِذَا ٱلْجِبَالُ نُسِفَتْ",
                "number": "",
                "verse": "10",
                "audioID": "v10",
                "url": "077010.mp3"
            },
            {
                "text": "وَإِذَا ٱلرُّسُلُ أُقِّتَتْ",
                "number": "",
                "verse": "11",
                "audioID": "v11",
                "url": "077011.mp3"
            },
            {
                "text": "لِأَىِّ يَوْمٍ أُجِّلَتْ",
                "number": "",
                "verse": "12",
                "audioID": "v12",
                "url": "077012.mp3"
            },
            {
                "text": "لِيَوْمِ ٱلْفَصْلِ",
                "number": "",
                "verse": "13",
                "audioID": "v13",
                "url": "077013.mp3"
            },
            {
                "text": "وَمَآ أَدْرَىٰكَ مَا يَوْمُ ٱلْفَصْلِ",
                "number": "",
                "verse": "14",
                "audioID": "v14",
                "url": "077014.mp3"
            },
            {
                "text": "وَيْلٌۭ يَوْمَئِذٍۢ لِّلْمُكَذِّبِينَ",
                "number": "",
                "verse": "15",
                "audioID": "v15",
                "url": "077015.mp3"
            },
            {
                "text": "أَلَمْ نُهْلِكِ ٱلْأَوَّلِينَ",
                "number": "",
                "verse": "16",
                "audioID": "v16",
                "url": "077016.mp3"
            },
            {
                "text": "ثُمَّ نُتْبِعُهُمُ ٱلْءَاخِرِينَ",
                "number": "",
                "verse": "17",
                "audioID": "v17",
                "url": "077017.mp3"
            },
            {
                "text": "كَذَٰلِكَ نَفْعَلُ بِٱلْمُجْرِمِينَ",
                "number": "",
                "verse": "18",
                "audioID": "v18",
                "url": "077018.mp3"
            },
            {
                "text": "وَيْلٌۭ يَوْمَئِذٍۢ لِّلْمُكَذِّبِينَ",
                "number": "",
                "verse": "19",
                "audioID": "v19",
                "url": "077019.mp3"
            },
            {
                "text": "أَلَمْ نَخْلُقكُّم مِّن مَّآءٍۢ مَّهِينٍۢ",
                "number": "",
                "verse": "20",
                "audioID": "v20",
                "url": "077020.mp3"
            },
            {
                "text": "فَجَعَلْنَٰهُ فِى قَرَارٍۢ مَّكِينٍ",
                "number": "",
                "verse": "21",
                "audioID": "v21",
                "url": "077021.mp3"
            },
            {
                "text": "إِلَىٰ قَدَرٍۢ مَّعْلُومٍۢ",
                "number": "",
                "verse": "22",
                "audioID": "v22",
                "url": "077022.mp3"
            },
            {
                "text": "فَقَدَرْنَا فَنِعْمَ ٱلْقَٰدِرُونَ",
                "number": "",
                "verse": "23",
                "audioID": "v23",
                "url": "077023.mp3"
            },
            {
                "text": "وَيْلٌۭ يَوْمَئِذٍۢ لِّلْمُكَذِّبِينَ",
                "number": "",
                "verse": "24",
                "audioID": "v24",
                "url": "077024.mp3"
            },
            {
                "text": "أَلَمْ نَجْعَلِ ٱلْأَرْضَ كِفَاتًا",
                "number": "",
                "verse": "25",
                "audioID": "v25",
                "url": "077025.mp3"
            },
            {
                "text": "أَحْيَآءًۭ وَأَمْوَٰتًۭا",
                "number": "",
                "verse": "26",
                "audioID": "v26",
                "url": "077026.mp3"
            },
            {
                "text": "وَجَعَلْنَا فِيهَا رَوَٰسِىَ شَٰمِخَٰتٍۢ وَأَسْقَيْنَٰكُم مَّآءًۭ فُرَاتًۭا",
                "number": "",
                "verse": "27",
                "audioID": "v27",
                "url": "077027.mp3"
            },
            {
                "text": "وَيْلٌۭ يَوْمَئِذٍۢ لِّلْمُكَذِّبِينَ",
                "number": "",
                "verse": "28",
                "audioID": "v28",
                "url": "077028.mp3"
            },
            {
                "text": "ٱنطَلِقُوٓا۟ إِلَىٰ مَا كُنتُم بِهِۦ تُكَذِّبُونَ",
                "number": "",
                "verse": "29",
                "audioID": "v29",
                "url": "077029.mp3"
            },
            {
                "text": "ٱنطَلِقُوٓا۟ إِلَىٰ ظِلٍّۢ ذِى ثَلَٰثِ شُعَبٍۢ",
                "number": "",
                "verse": "30",
                "audioID": "v30",
                "url": "077030.mp3"
            },
            {
                "text": "لَّا ظَلِيلٍۢ وَلَا يُغْنِى مِنَ ٱللَّهَبِ",
                "number": "",
                "verse": "31",
                "audioID": "v31",
                "url": "077031.mp3"
            },
            {
                "text": "إِنَّهَا تَرْمِى بِشَرَرٍۢ كَٱلْقَصْرِ",
                "number": "",
                "verse": "32",
                "audioID": "v32",
                "url": "077032.mp3"
            },
            {
                "text": "كَأَنَّهُۥ جِمَٰلَتٌۭ صُفْرٌۭ",
                "number": "",
                "verse": "33",
                "audioID": "v33",
                "url": "077033.mp3"
            },
            {
                "text": "وَيْلٌۭ يَوْمَئِذٍۢ لِّلْمُكَذِّبِينَ",
                "number": "",
                "verse": "34",
                "audioID": "v34",
                "url": "077034.mp3"
            },
            {
                "text": "هَٰذَا يَوْمُ لَا يَنطِقُونَ",
                "number": "",
                "verse": "35",
                "audioID": "v35",
                "url": "077035.mp3"
            },
            {
                "text": "وَلَا يُؤْذَنُ لَهُمْ فَيَعْتَذِرُونَ",
                "number": "",
                "verse": "36",
                "audioID": "v36",
                "url": "077036.mp3"
            },
            {
                "text": "وَيْلٌۭ يَوْمَئِذٍۢ لِّلْمُكَذِّبِينَ",
                "number": "",
                "verse": "37",
                "audioID": "v37",
                "url": "077037.mp3"
            },
            {
                "text": "هَٰذَا يَوْمُ ٱلْفَصْلِ ۖ جَمَعْنَٰكُمْ وَٱلْأَوَّلِينَ",
                "number": "",
                "verse": "38",
                "audioID": "v38",
                "url": "077038.mp3"
            },
            {
                "text": "فَإِن كَانَ لَكُمْ كَيْدٌۭ فَكِيدُونِ",
                "number": "",
                "verse": "39",
                "audioID": "v39",
                "url": "077039.mp3"
            },
            {
                "text": "وَيْلٌۭ يَوْمَئِذٍۢ لِّلْمُكَذِّبِينَ",
                "number": "",
                "verse": "40",
                "audioID": "v40",
                "url": "077040.mp3"
            },
            {
                "text": "إِنَّ ٱلْمُتَّقِينَ فِى ظِلَٰلٍۢ وَعُيُونٍۢ",
                "number": "",
                "verse": "41",
                "audioID": "v41",
                "url": "077041.mp3"
            },
            {
                "text": "وَفَوَٰكِهَ مِمَّا يَشْتَهُونَ",
                "number": "",
                "verse": "42",
                "audioID": "v42",
                "url": "077042.mp3"
            },
            {
                "text": "كُلُوا۟ وَٱشْرَبُوا۟ هَنِيٓـًٔۢا بِمَا كُنتُمْ تَعْمَلُونَ",
                "number": "",
                "verse": "43",
                "audioID": "v43",
                "url": "077043.mp3"
            },
            {
                "text": "إِنَّا كَذَٰلِكَ نَجْزِى ٱلْمُحْسِنِينَ",
                "number": "",
                "verse": "44",
                "audioID": "v44",
                "url": "077044.mp3"
            },
            {
                "text": "وَيْلٌۭ يَوْمَئِذٍۢ لِّلْمُكَذِّبِينَ",
                "number": "",
                "verse": "45",
                "audioID": "v45",
                "url": "077045.mp3"
            },
            {
                "text": "كُلُوا۟ وَتَمَتَّعُوا۟ قَلِيلًا إِنَّكُم مُّجْرِمُونَ",
                "number": "",
                "verse": "46",
                "audioID": "v46",
                "url": "077046.mp3"
            },
            {
                "text": "وَيْلٌۭ يَوْمَئِذٍۢ لِّلْمُكَذِّبِينَ",
                "number": "",
                "verse": "47",
                "audioID": "v47",
                "url": "077047.mp3"
            },
            {
                "text": "وَإِذَا قِيلَ لَهُمُ ٱرْكَعُوا۟ لَا يَرْكَعُونَ",
                "number": "",
                "verse": "48",
                "audioID": "v48",
                "url": "077048.mp3"
            },
            {
                "text": "وَيْلٌۭ يَوْمَئِذٍۢ لِّلْمُكَذِّبِينَ",
                "number": "",
                "verse": "49",
                "audioID": "v49",
                "url": "077049.mp3"
            },
            {
                "text": "فَبِأَىِّ حَدِيثٍۭ بَعْدَهُۥ يُؤْمِنُونَ",
                "number": "",
                "verse": "50",
                "audioID": "v50",
                "url": "077050.mp3"
            }

        
        ],
        "count": 50,
